<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 다른 mapper와 중복되지 않도록 네임스페이스 기재 -->
<mapper namespace="exhibition">
 <select id="list" resultType="com.ranzo.power.model.exhibition.dto.ExhibitionDTO">
  select * from EXHIBITION_TB
 </select>
 <select id="detail" resultType="com.ranzo.power.model.exhibition.dto.ExhibitionDTO">
 select * from EXHIBITION_TB
 where code=#{code}
 </select>

	
</mapper>














