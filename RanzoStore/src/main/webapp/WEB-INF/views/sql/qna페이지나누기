-- 우선 아래 각 테이블의 데이터를 지운다.
delete from qna_attach_tb;
delete from qna_tb;

--페이지 나누기 테스트를 위해 레코드 입력
declare --선언부
    i number := 1;
begin --실행부
    while i<=991 loop
        insert into qna_tb (bno,title,content,writer)
        values
        ( (select nvl(max(bno)+1,1) from qna_tb)
        ,'제목'||i, '내용'||i, 'kim');
        i := i+1; --조건 변경
    end loop;
end;
/

select * from qna_tb;

--레코드 갯수 확인
select count(*) from qna_tb;

commit;

select *
			from (
			    select rownum as rn, A.*
			    from (
			        select bno,title,writer,name,content,regdate,viewcnt
			        from qna_tb q, member_tb m
			        where q.writer=m.userid
			        order by bno desc 
			    ) A    
			) where rn between #{start} and #{end}



