<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 다른 mapper와 중복되지 않도록 네임스페이스 기재 -->
<mapper namespace="qna">
	<select id="listAll" resultType="QnaDTO">
		select bno, title, writer, reg_date, viewcnt
		from qna_tb q, member_tb m
		where q.writer = m.userid
		order by bno desc 
	</select>
	
	<insert id="insert">
	insert into qna_tb (bno,title,content,writer) values
  (seq_qna.nextval,#{title},#{content},#{writer})
	</insert>
</mapper>














